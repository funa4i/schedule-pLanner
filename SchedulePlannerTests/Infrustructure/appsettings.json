{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "../logs/electrical-.log",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} {CorrelationId} {Level:u3} {Username} {Message:lj}{Exception}{NewLine}"
      }
    }]
  },
  "Jwt": {
    "Issuer": "AuthServer",
    "Audience": "AuthClient",
    "Key": "secretsecretsecretkey!1231231231123"
  },
  "AllowedHosts": "*",
  "SalarySettings": {
    "ExtraSaleSum": 1000
  },
  "ThreadSettings": {
    "MaxCount": 10
  }
}